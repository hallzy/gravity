#unittest {
	name: "Complex splitAfter() Test";
	error: NONE;
	result: true;
};

func main () {
	var s = "1.02.03.04.05.06.07.08.09.010.0"
  var list = s.splitAfter(".0")

	var ret = true;

	var expected = 0
	list.loop( func(value) {
		expected += 1
		ret = ret and value == expected.String() + ".0"
	})

	// Check expected just to make sure the loop actually ran
	return ret and expected == 10
}
