#unittest {
	name: "Basic splitAfter() Test";
	error: NONE;
	result: true;
};

func main () {
	var s = "102030405060708090"
  var list = s.splitAfter("0")

	var ret = true;

	var expected = 0
	list.loop( func(value) {
		expected += 10
		ret = ret and value == expected.String()
	})

	// Check expected just to make sure the loop actually ran
	return ret and expected == 90
}
